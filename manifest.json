{
  "author": "igorlogius",
  "browser_action": {
    "theme_icons": [
      {
        "light": "dark-256.png",
        "dark": "light-256.png",
        "size": 256
      },
      {
        "light": "dark-128.png",
        "dark": "light-128.png",
        "size": 128
      },
      {
        "light": "dark-64.png",
        "dark": "light-64.png",
        "size": 64
      },
      {
        "light": "dark-32.png",
        "dark": "light-32.png",
        "size": 32
      },
      {
        "light": "dark-16.png",
        "dark": "light-16.png",
        "size": 16
      }
    ]
  },
  "background": {
	"scripts": ["background.js"]
  },
  "commands": {
    "_execute_browser_action": {
        "suggested_key": {
            "default": "Ctrl+Alt+B"
        }
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Bookmark Router",
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": [
    "menus",
    "tabs",
    "storage",
    "bookmarks"
  ],
  "version": "1.5.7"
}
