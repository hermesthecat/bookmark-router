{
  "author": "igorlogius",
  "browser_action": {
  },
  "background": {
	"scripts": ["background.js"]
  },
  "commands": {
    "_execute_browser_action": {
        "suggested_key": {
            "default": "Ctrl+Alt+B"
        }
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Bookmark Router",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "menus",
    "storage",
    "bookmarks",
    "clipboardWrite"
  ],
  "version": "1.3.6"
}
